    private void salirLblMouseEntered(java.awt.event.MouseEvent evt) {                                      

        salirLbl.setOpaque(true);
        salirLbl.setBackground(Color.red);
        salirLbl.setForeground(Color.white);
    }                                     

    private void salirLblMouseExited(java.awt.event.MouseEvent evt) {                                     
 
        salirLbl.setOpaque(false);
        salirLbl.setForeground(Color.black);
    }                                    

    private void lb_ajustesMouseClicked(java.awt.event.MouseEvent evt) {                                        

        this.ajustess = new Ajustes(this);
        ajustess.setVisible(true);
        this.dispose();
        
    }                                       

